apiVersion: v1
kind: Service
metadata:
  labels:
    app: bns-fullnode-seed
    component: core
  name: bns-fullnode-seed
spec:
  ports:
  - name: rpc
    port: 26657
  selector:
    app: bns-fullnode-seed
---
apiVersion: apps/v1
kind: StatefulSet
metadata:
  labels:
    app: bns-fullnode-seed
    component: core
  name: bns-fullnode-seed
spec:
  podManagementPolicy: Parallel
  replicas: 1
  selector:
    matchLabels:
      app: bns-fullnode-seed
  serviceName: bns-fullnode-seed
  template:
    metadata:
      labels:
        app: bns-fullnode-seed
        clusterName: bns
        component: core
    spec:
      containers:
      - args:
        - -home=/data/bns
        - start
        - -bind=unix:///socks/app.sock
        - -min_fee=0.0001 IOV
        image: iov1/bnsd:v0.21.0
        imagePullPolicy: IfNotPresent
        name: bns
        securityContext:
          readOnlyRootFilesystem: true
        volumeMounts:
        - mountPath: /socks
          name: socksdir
        - mountPath: /data/bns
          name: tmdir
          subPath: bns
      - command:
        - bash
        - -c
        - |
          set -ex
          exec tendermint node  \
            --moniker="`hostname`" \
            --log_level="p2p:info,state:info,*:error" \
            --proxy_app="unix:///socks/app.sock"
        image: iov1/tendermint:v0.31.5
        imagePullPolicy: IfNotPresent
        name: tendermint
        ports:
        - containerPort: 26656
          name: p2p
        - containerPort: 26657
          name: rpc
        resources:
          limits:
            cpu: 1100m
            memory: 2.5Gi
          requests:
            cpu: 750m
            memory: 1Gi
        securityContext:
          readOnlyRootFilesystem: true
        volumeMounts:
        - mountPath: /data/tendermint/config/config.toml
          name: tm-configuration
          readOnly: true
          subPath: config.toml
        - mountPath: /data/tendermint/config/genesis.json
          name: tm-genesis
          readOnly: true
          subPath: genesis.json
        - mountPath: /data/tendermint
          name: tmdir
          subPath: tendermint
        - mountPath: /socks
          name: socksdir
        - mountPath: /tmp
          name: tm-rw-volume
      initContainers:
      - args:
        - |
          set -exo pipefail
          if [ ! -f ${TMHOME}/.INITIALISED ]; then
            mkdir -p /data/bns
            tendermint init
            touch ${TMHOME}/.INITIALISED
          fi
          rm -f /socks/app.sock
        command:
        - bash
        - -c
        env:
        - name: TMHOME
          value: /data/tendermint
        image: iov1/tendermint:v0.31.5
        imagePullPolicy: IfNotPresent
        name: tm-init
        resources:
          limits:
            cpu: 150m
            memory: 150Mi
          requests:
            cpu: 100m
            memory: 100Mi
        securityContext:
          readOnlyRootFilesystem: true
        volumeMounts:
        - mountPath: /data
          name: tmdir
        - mountPath: /socks
          name: socksdir
      restartPolicy: Always
      terminationGracePeriodSeconds: 20
      volumes:
      - configMap:
          name: seed-tendermint-config-8ct5h44kc5
        name: tm-configuration
      - configMap:
          name: tm-seed-init-config-6thbbdct5m
        name: tm-genesis
      - emptyDir: {}
        name: socksdir
      - emptyDir: {}
        name: tm-rw-volume
  updateStrategy:
    type: RollingUpdate
  volumeClaimTemplates:
  - metadata:
      name: tmdir
    spec:
      accessModes:
      - ReadWriteOnce
      resources:
        requests:
          storage: 1Gi
      storageClassName: standard
